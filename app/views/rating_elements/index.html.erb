<p id="notice"><%= notice %></p>

<h1>Listing Rating Elements</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Score</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rating_elements.each do |rating_element| %>
      <tr>
        <td><%= rating_element.title %></td>
        <td><%= rating_element.score %></td>
        <td><%= link_to 'Show', rating_element %></td>
        <td><%= link_to 'Edit', edit_rating_element_path(rating_element) %></td>
        <td><%= link_to 'Destroy', rating_element, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rating element', new_rating_element_path %>
